      "fontWeight": {
        "body": 300,
        "h1": 600,
        "h2": 600,
        "h3": 500,
        "h4": 400,
        "h5": 400,
        "h6": 300,
        "thin": 100,
        "extra-light": 200,
        "light": 300,
        "regular": 400,
        "medium": 500,
        "semi-bold": 600,
        "bold": 700,
        "extra-bold": 800,
        "black": 900
