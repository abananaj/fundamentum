{
  "version": 3,
  "$schema": "https://schemas.wp.org/trunk/theme.json",

  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "layout": {
      "allowCustomContentAndWideSize": true,
      "allowEditing": true,
      "contentSize": "1140px",
      "wideSize": "100%"
    },
    "lightbox": {
      "allowEditing": true,
      "enabled": true
    },
    "position": {
      "sticky": true
    },
    "shadow": {
      "defaultPresets": true,
      "presets": [
        {
          "name": "Soft pop",
          "slug": "soft-pop",
          "shadow": "4.5px 14.6px 18.1px -8px rgba(0, 0, 0, 0.024), 12.5px 40.3px 50px -8px rgba(0, 0, 0, 0.15), 30.1px 97.1px 120.3px -8px rgba(0, 0, 0, 0.016), 100px 322px 399px -8px rgba(0, 0, 0, 0.02);"
        },
        {
          "name": "floating",
          "slug": "floating",
          "shadow": "box-shadow: 8px 8px 14px -6px rgba(0, 0, 0, 0.12)"
        }
      ]
    },
    "spacing": {
      "blockGap": true,
      "customSpacingSize": true,
      "margin": true,
      "padding": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "spacingScale": {
        "increment": 1.618,
        "mediumStep": 1,
        "operator": "*",
        "steps": 7,
        "unit": "rem"
      }
    },
    "color": {
      "background": true,
      "button": true,
      "caption": true,
      "heading": true,
      "link": true,
      "text": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
