      "fontSizes": [
        {
          "slug": "h1",
          "size": "4.26rem",
          "name": "Heading 1"
        },
        {
          "slug": "h2",
          "size": "2.62rem",
          "name": "Heading 2"
        },
        {
          "slug": "h3",
          "size": "1.618rem",
          "name": "Heading 3"
        },
        {
